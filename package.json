{
	"name": "styocss-monorepo",
	"type": "module",
	"version": "0.2.0",
	"private": true,
	"packageManager": "pnpm@8.6.0",
	"description": "The instant on-demand atomic css-in-js engine.",
	"author": "DevilTea <ch19980814@gmail.com>",
	"license": "MIT",
	"keywords": [
		"styocss",
		"atomic-css",
		"css-in-js",
		"atomic-css-in-js-engine"
	],
	"scripts": {
		"build": "pnpm -r --filter=./packages/* run build && pnpm -r --filter=./packages/* exec publint",
		"release": "pnpm exec bumpp -r && pnpm run build && pnpm -r --filter=./packages/* publish",
		"calcsize": "tsx ./scripts/calcsize.ts",
		"lint": "eslint --fix .",
		"lint:packages": "eslint --fix ./packages/**/src/*",
		"lint:scripts": "eslint --fix ./scripts/*",
		"lint:configs": "eslint --fix ./alias.ts ./vitest.config.ts",
		"publint": "pnpm -r --filter=./packages/* exec publint",
		"test:unit": "vitest",
		"test:coverage": "vitest run --coverage"
	},
	"devDependencies": {
		"@deviltea/eslint-config": "3.1.0",
		"@deviltea/tsconfig": "0.0.5",
		"@types/inquirer": "9.0.7",
		"@types/node": "18.14.2",
		"@vitest/coverage-v8": "1.4.0",
		"brotli-size": "4.0.0",
		"bumpp": "9.4.0",
		"eslint": "8.57.0",
		"fast-glob": "3.3.2",
		"gzip-size": "7.0.0",
		"happy-dom": "13.8.6",
		"inquirer": "9.2.16",
		"publint": "0.2.7",
		"rimraf": "5.0.5",
		"terser": "5.29.2",
		"tsup": "8.0.2",
		"tsx": "4.7.1",
		"typescript": "5.4.2",
		"vite": "5.2.2",
		"vitest": "1.4.0"
	}
}
