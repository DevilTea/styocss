{
  "name": "@styocss/helpers",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.9",
  "author": "DevilTea <ch19980814@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevilTea/styocss.git",
    "directory": "packages/helpers"
  },
  "bugs": {
    "url": "https://github.com/DevilTea/styocss/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf ./dist ./.tsbuildinfo && pnpm build:temp-dts && pnpm build:tsup && rimraf ./dist/temp ./.tsbuildinfo",
    "build:temp-dts": "tsc -p ../../tsconfig.helpers.json --declarationDir ./dist/temp --declaration --emitDeclarationOnly",
    "build:tsup": "tsup"
  }
}
