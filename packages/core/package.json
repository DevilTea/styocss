{
  "name": "@styocss/core",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.8",
  "author": "DevilTea <ch19980814@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevilTea/styocss.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/DevilTea/styocss/issues"
  },
  "keywords": [
    "styocss",
    "atomic-css",
    "css-in-js",
    "atomic-css-in-js-engine"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf ./dist ./.tsbuildinfo && pnpm build:temp-dts && pnpm build:tsup && rimraf ./dist/temp ./.tsbuildinfo",
    "build:temp-dts": "tsc -p ../../tsconfig.core.json --declarationDir ./dist/temp --declaration --emitDeclarationOnly",
    "build:tsup": "tsup"
  },
  "dependencies": {
    "csstype": "^3.1.1"
  }
}
